{
  "name": "lb4-connector-dynamodb",
  "description": "DynamoDB connector for LoopBack 4",
  "version": "0.2.6-4",
  "main": "index.js",
  "scripts": {
    "test": "env NODE_ENV=test mocha --color test/*.test.js --timeout 2000 --reporter spec",
    "tdd": "env NODE_ENV=test mocha --color -w --recursive",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint ."
  },
  "dependencies": {
    "async": "latest",
    "aws-sdk": "latest",
    "colors": "latest",
    "loopback-connector": "^4.7.0",
    "strong-globalize": "^5.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.4",
    "dynalite": "^2.3.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "jugglingdb": ">= 0.1.0",
    "mocha": "latest",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.4.1",
    "should": "latest"
  },
  "nyc": {
    "exclude": [
      "server/boot",
      "coverage",
      "test"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sam016/lb4-connector-dynamodb/"
  },
  "author": {
